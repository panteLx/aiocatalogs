{
  "name": "aiocatalogs",
  "version": "2.0.0",
  "description": "AIOCatalogs is an open-source addon that combines multiple catalog addons into a single one with enhanced performance and a user-friendly configuration interface. It features multi-platform compatibility, serverless deployment options, and an optimized caching system. Join our community on Discord (discord.gg/Ma4SnagqwE) or contribute on GitHub (github.com/panteLx/aiocatalogs)",
  "private": true,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/panteLx/aiocatalogs.git"
  },
  "keywords": [
    "stremio",
    "addon",
    "catalogs",
    "catalog",
    "streaming",
    "movies",
    "series",
    "media",
    "entertainment",
    "unified catalog",
    "catalog manager",
    "stremio addons",
    "streaming catalogs"
  ],
  "author": "panteLx",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/panteLx/aiocatalogs/issues"
  },
  "homepage": "https://github.com/panteLx/aiocatalogs#readme",
  "scripts": {
    "db:up": "pnpm with-env drizzle-kit up",
    "db:generate": "pnpm with-env drizzle-kit generate",
    "db:push": "pnpm with-env drizzle-kit push",
    "db:push:local": "pnpm with-env pnpm with-local-db drizzle-kit push --config drizzle.local.config.ts",
    "db:migrate": "pnpm with-env drizzle-kit migrate",
    "db:migrate:local": "pnpm with-env pnpm with-local-db drizzle-kit migrate --config drizzle.local.config.ts",
    "db:check": "pnpm with-env drizzle-kit check",
    "db:studio": "pnpm with-env drizzle-kit studio",
    "db:studio:local": "pnpm with-env pnpm with-local-db drizzle-kit studio --config drizzle.local.config.ts",
    "d1:migrate:local": "wrangler d1 migrations apply aiocatalogs-v2 --local",
    "d1:migrate:remote": "wrangler d1 migrations apply aiocatalogs-v2 --remote",
    "dev": "pnpm with-env next dev",
    "build": "pnpm with-env next build",
    "lint": "pnpm with-env next lint",
    "dev:worker": "wrangler dev",
    "build:worker": "pnpm with-env cloudflare",
    "preview": "pnpm build:worker && wrangler versions upload",
    "deploy": "pnpm build:worker && wrangler deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts",
    "bindings": "cf-bindings-proxy",
    "with-env": "dotenv -e .dev.vars --",
    "with-local-db": "cross-env LOCAL_DB_PATH=$(find .wrangler/state/v3/d1/miniflare-D1DatabaseObject -type f -name '*.sqlite' -print -quit)"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@radix-ui/react-hover-card": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-toast": "^1.2.14",
    "@t3-oss/env-nextjs": "^0.10.1",
    "@tanstack/react-query": "^5.50.0",
    "@trpc/client": "^11.0.0-rc.446",
    "@trpc/next": "^11.0.0-rc.446",
    "@trpc/react-query": "^11.0.0-rc.446",
    "@trpc/server": "^11.0.0-rc.446",
    "cf-bindings-proxy": "^0.7.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.30.10",
    "geist": "^1.3.1",
    "lucide-react": "^0.511.0",
    "nanoid": "^5.1.5",
    "next": "14.1.0",
    "next-themes": "^0.4.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.57.0",
    "react-markdown": "^10.1.0",
    "server-only": "^0.0.1",
    "sonner": "^2.0.4",
    "superjson": "^2.2.1",
    "tailwind-merge": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.25.46"
  },
  "devDependencies": {
    "@libsql/client": "^0.7.0",
    "@opennextjs/cloudflare": "^0.1.0",
    "@types/eslint": "^8.56.10",
    "@types/node": "^20.17.57",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.4.2",
    "drizzle-kit": "^0.21.4",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.1.0",
    "eslint-plugin-drizzle": "^0.2.3",
    "postcss": "^8.4.39",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.5.3",
    "vercel": "^35.1.0",
    "wrangler": "^4.18.0"
  },
  "ct3aMetadata": {
    "initVersion": "7.36.1"
  },
  "packageManager": "pnpm@9.5.0"
}
